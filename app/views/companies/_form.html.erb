<% if @company.errors.any? %>
  <div id="error_explanation">
    <h2>Whoops! There were <%= pluralize(@company.errors.count, "error") %> creating your account:</h2>
    <ul>
    <% @company.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>

<%= form_for(@company) do |f| %>
<!-- <p>Company Name <%= f.text_field :companyname %></p> -->

<p>
  <%= f.label "Company" %>
  <%= autocomplete_field_tag 'company', "", autocomplete_company_companyname_path, placeholder: "Company Name", size: 30 %>
</p>

<p>Website <%= f.text_field :website, placeholder: "www.highfivejobs.com", size: 30 %></p>
<p>Industry <%= f.text_field :industry %></p>
<p>Company Notes<%= f.text_field :companynotes %></p>
<%= f.submit %>
<% end %>