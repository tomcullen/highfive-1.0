<h1><%= @company.companyname %></h1>

<p>
  <%= link_to @company.website, "http://#{@company.website}" unless @company.website.blank? %>
</p>
<p><%= @company.industry %></p>
<p><%= @company.companynotes %></p>

<h3>My <%= @company.companyname %> Contacts:</h3>
<p><% @company.contacts.each do |contact| %>
  <ul>  
    <%= contact.firstname %>
    <%= contact.lastname %>
  </ul>
<% end %>
</p>  
<p><%= link_to "Edit Company", edit_company_url %> | 
<%= link_to "Delete Comany", company_url, method: :delete %></p>
  